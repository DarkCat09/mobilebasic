SUB MAIN
  PRINT "14 + 2 = " + STRING(14 + 2)
  PRINT "14 - 2 = " + STRING(14 - 2)
  PRINT "14 * 2 = " + STRING(14 * 2)
  PRINT "14 / 2 = " + STRING(14 / 2)
  PRINT "157 % 100 = " + STRING(157 % 100)
  PRINT "10.0 ** 2.0 = " + STRING(10.0 ** 2.0)
  PRINT "5 * -3 = " + STRING(5 * -3)
  
  PRINT "3 * 4 + 2 = " + STRING(3*4+2)
  PRINT "(3 * 4) + 2 = " + STRING((3*4)+2)
  PRINT "3 * (4 + 2) = " + STRING(3*(4+2))
END SUB
